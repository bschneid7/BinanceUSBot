{
  "name": "BinanceUSBot",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run shared-install && concurrently -n \"client,server\" \"npm run client-install\" \"npm run server-install\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrently -n \"shared,client,server\" \"npm run shared-dev\" \"npm run client\" \"npm run server\"",
    "client": "cd client && npm run dev",
    "client-install": "cd client && npm install",
    "server": "cd server && npm run dev",
    "server-install": "cd server && npm install",
    "shared-install": "cd shared && npm install && npm run build",
    "shared-build": "cd shared && npm run build",
    "shared-dev": "cd shared && npm run dev",
    "start": "concurrently -n \"client,server\" \"npm run client\" \"npm run server\"",
    "lint": "concurrently -n \"client,server,shared\" \"cd client && npm run lint\" \"cd server && npm run lint\" \"cd shared && npm run lint\"",
    "debug": "concurrently -n \"client,server\" \"npm run client\" \"cross-env NODE_OPTIONS=--inspect-brk=9229 npm run server\"",
    "seed": "cd server && npm run seed",
    "seed:positions": "cd server && npm run seed:positions",
    "seed:user": "cd server && npm run seed:user",
    "seed:admin": "cd server && tsx scripts/db-seed-admin.ts",
    "db:cleanup": "cd server && tsx scripts/db-cleanup.ts",
    "db:reset": "cd server && tsx scripts/db-reset.ts",
    "api:test": "tsx scripts/api-test.ts",
    "train:ppo": "cd server && npm run train:ppo",
    "train:ml": "cd server && tsx scripts/trainMLModel.ts",
    "evaluate:ml": "cd server && tsx scripts/evaluateMLPerformance.ts",
    "seed:ml": "cd server && tsx scripts/seedMLTrainingData.ts",
    "stake:idle": "tsx scripts/stakeIdle.ts",
    "tax:generate": "tsx scripts/generateTax.ts",
    "build": "npm run shared-build && cd server && npm run build && cd ../client && npm run build",
    "build:docker": "docker compose build",
    "deploy": "bash deploy.sh",
    "deploy:docker": "bash scripts/docker-deploy.sh"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@tensorflow/tfjs-node": "^4.22.0",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^7.0.3",
    "axios": "^1.13.1",
    "concurrently": "^9.1.0",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.10",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "tsx": "^4.20.6"
  }
}
